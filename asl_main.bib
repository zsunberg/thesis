}

@Article{LM-DA:00b,
  Title                    = {{O}ptimization of discontinuous functions: a generalized theory of differentiation},
  Author                   = {{L}. {M}oreau and {D}. {A}eyels},
  Journal                  = sicon,
  Year                     = {2000},
  Number                   = {1},
  Pages                    = {53-69},
  Volume                   = {11},

  Abstract                 = {Problems of optimization and optimal control with discontinuous costs are considered. For that purpose, we introduce some preliminary ideas of a new generalized theory of differentiation, the main ideas of which are inspired by the work of F.H. Clarke (Classics in Appl. Math., vol. 5, 1990). We present two calculus rules and apply the introduced theory to the study of some optimization and optimal control problems with discontinuous costs. (11 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

@Article{LM-DA:00c,
  Title                    = {{A}symptotic methods in the stability analysis of parametrized homogeneous flows},
  Author                   = {{L}. {M}oreau and {D}. {A}eyels},
  Journal                  = automatica,
  Year                     = {2000},
  Number                   = {8},
  Pages                    = {1213-18},
  Volume                   = {36},

  Abstract                 = {We consider a collection of flows phi /sup epsilon / on R/sup n/ labeled by a parameter epsilon >or=0. It is assumed that trajectories of phi /sup epsilon / converge-uniformly on compact time intervals-to trajectories of phi /sup 0/ as epsilon down arrow 0. Extra conditions are determined under which, if phi /sup 0/ has an asymptotically stable equilibrium point, then phi /sup epsilon / has an asymptotically stable equilibrium point. Examples illustrating the theory are included. (10 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

