@Article{RWB:76b,
  Title                    = {{V}olterra {S}eries and {G}eometric {C}ontrol {T}heory},
  Author                   = {{R}. {W}. {B}rockett},
  Journal                  = automatica,
  Year                     = {1976},
  Number                   = {2},
  Pages                    = {167-176},
  Volume                   = {12},

  Abstract                 = {It is shown that controlled differential equations which are analytic in the state and linear in the control have solutions which can be expanded in a Volterra series provided there is no finite escape time. The Volterra kernels are computed in terms of the power series expansion of the functions defining the differential equation. Necessary and sufficient conditions for a Volterra series to be realizable by a linear-analytic system are particularly easy to test if the Volterra series is finite; a complete theory is worked out for this instance. Applications are considered to singular control, multilinear realization theory, etc. (15 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:11 +0000}
}

@Article{RWB:73,
  Title                    = {{L}ie {T}heory and {C}ontrol {S}ystems defined on {S}pheres},
  Author                   = {{R}. {W}. {B}rockett},
  Journal                  = siamJAM,
  Year                     = {1973},
  Number                   = {2},
  Pages                    = {213-225},
  Volume                   = {25},

  Abstract                 = {Shows that in constructing a theory for the most elementary class of control problems defined on spheres, some results from Lie theory play a natural role. In particular to understand controllability, optimal control, and certain properties of stochastic equations. Lie theoretic ideas are needed. The framework considered here is probably the most natural departure from the usual linear system/vector space problems which have dominated the control systems literature. For this reason the results are compared with those previously available for the finite-dimensional vector space case. (17 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:11 +0000}
}

@Article{RWB:72,
  Title                    = {{S}ystem {T}heory on {G}roup {M}anifolds and {C}oset {S}paces},
  Author                   = {{R}. {W}. {B}rockett},
  Journal                  = siamJC,
  Year                     = {1972},
  Number                   = {2},
  Pages                    = {265-284},
  Volume                   = {10},

  Abstract                 = {The purpose of this paper is to study questions regarding controllability, observability, and realization theory for a particular class of systems for which the state space is a differentiable manifold which is simultaneously a group or, more generally, a coset space. It is shown that it is possible to give rather explicit expressions for the reachable set and the set of indistinguishable states in the case of autonomous systems. A type of state space isomorphism theorem is also established. These results parallel, and in part specialize to, results available for the familiar case described by dx(t)/dt=Ax(t)+Bu(t), y(t)=Cx(t). The objective is to reduce all questions about the system to questions about Lie algebras generated from the coefficient matrices entering in the description of the system and in that way arrive at conditions which are easily visualized and tested. (20 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:11 +0000}
}

