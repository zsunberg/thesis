  Abstract                 = {This note points out that in a wide class of forced relaxation oscillators the hyperbolic behavior, which has up to now been known to exist only on small set, dominates in the Lebesgue sense in a wide class of relaxation oscillators. This note introduces the simplest physically realistic smooth system where the strange attractor is expected to exist for most (in the Lebesgue sense) parameter values. This phenomenon went unobserved for over half a century since the first work on forced relaxation oscillations by Cartwright, Littlewood and Levinson. (15 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

@Article{ML:96,
  Title                    = {{C}omposition of rotations and parallel transport},
  Author                   = {{M}. {L}evi},
  Journal                  = {{N}onlinearity},
  Year                     = {1996},
  Number                   = {2},
  Pages                    = {413-19},
  Volume                   = {9},

  Abstract                 = {This note provides the details and proofs of the results announced by Levi (1993 Fields Institute Communications vol. 1, pp. 133-8). The main result of this note is a geometrical representation of the reconstruction problem for SO(3) in terms of parallel transport. It is, of course, well known that the solution of a linear equation x= Omega (t)x in R/sup n/ cannot in general be expressed by x(t)=e( integral /sub 0//sup t/)/sup Omega ( tau /d tau )x(0) (false) because the coefficient matrices Omega (t/sub 1/) and Omega (t/sub 2/) may fail to commute for t/sub 1/ not=t/sub 2/. Nevertheless, when n=3 and when Omega (t) is skew-symmetric, i.e. when it lies in the Lie algebra of the group of rigid rotations in R/sup 3/, the above false formula is almost correct, as we will show here. The main result of this note is a geometrical expression for the matrix solution X(t) of matrix equations on TSO(3) of the form X= Omega (t)X, Omega /sup T/=- Omega (*) where X, Omega are 3*3 matrices with real coefficients. The argument relies on a theorem of Poinsot together with some observations on geodesic curvatures of moving curves. (18 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:09 +0000}
}

@Article{ML:93,
  Title                    = {{G}eometric phases in the motion of rigid bodies},
  Author                   = {{M}. {L}evi},
  Journal                  = arma,
  Year                     = {1993},
  Number                   = {3},
  Pages                    = {213-29},
  Volume                   = {122},

  Abstract                 = {The author makes some differential geometric observations on the kinematics of convex surfaces rolling along a fixed plane in R/sup 3/, and on the relationship of the problem with parallel transport and the Gauss-Bonnet formula. These ideas are then applied to recover 'Berry's phase' of a free rigid body which was found by Montgomery (1991) using Stokes' theorem. He also points out a new 'twist' on this problem. As a second application, he gives a solution of a problem posed by R. Brockett. As a third application, he gives a geometrical description of 'Berry's phase' in SO(3); this can be applied to various rigid-elastic systems to compute their geometric phases. (16 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:09 +0000}
}

@Article{ML:97,
