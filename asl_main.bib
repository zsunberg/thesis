@Article{SL:84,
  Title                    = {{S}ur les trajectoires du gradient d'une fonction analytique},
  Author                   = {{S}. {\L}ojasiewicz},
  Journal                  = {{S}eminari di {G}eometria 1982-1983},
  Year                     = {1984},
  Note                     = {Istituto di Geometria, Dipartimento di Matematica, Universit{\`a} di Bologna, Italy},
  Pages                    = {115-117},

  Amsabstract              = {From the text (translated from the French): "Theorem: Let $f$ be an analytic function $\ge0$ in a neighborhood of 0 in $R^n$ such that $f(0)=0$. Then there exists a neighborhood $U$ of zero such that every trajectory $y_x$ of the system $y'=-{\rm grad} \,f(y)$ satisfying $y_x(0)=x$ is defined in $[0,\infty)$, has finite length and tends to a point of $Z=\{f(x)=0\}$ as $t\rightarrow \infty$, with uniform convergence. We need the following inequality: If $F$ is an analytic function in a neighborhood of 0 in $R^n$ such that $F(0)=0$, then there exists $0<\zeta<1$ such that $|{\rm grad}\,F(x)|\ge|F(x)|^\zeta$ in a neighborhood of zero. Remark: The theorem implies that every analytic set is (locally) a retract under deformation of its neighborhood."},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:10 +0000},
  Fbnote                   = {Lojasiewicz Gradient Inequality}
