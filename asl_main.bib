@Article{RWB:91,
  Title                    = {{D}ynamical systems that sort lists, diagonalize matrices, and solve linear programming problems},
  Author                   = {{R}. {W}. {B}rockett},
  Journal                  = laia,
  Year                     = {1991},
  Pages                    = {79-91},
  Volume                   = {146},

  Abstract                 = {We establish a number of properties associated with the dynamical system $\dot H=[H,[H,N]]$, where $H$ and $N$ are symmetric $n$ by $n$ matrices and $[A,B]=AB-BA$. The most important of these come from the fact that this equation is equivalent to a certain gradient flow on the space of orthogonal matrices. We are especially interested in the role of this equation as an analog computer. For example, we show how to map the data associated with a linear programming problem into $H(0)$ and $N$ in such a way as to have $\dot H=[H,[H,N]]$ evolve to a solution of the linear programming problem. This result can be applied to find systems which solve a variety of generic combinatorial optimization problems, and it even provides an algorithm for diagonalizing symmetric matrices."},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:10 +0000}
}

@Article{RWB:89,
  Title                    = {{O}n the rectification of vibratory motion},
  Author                   = {{R}. {W}. {B}rockett},
  Journal                  = {{S}ensors \& {A}ctuators},
  Year                     = {1989},
  Number                   = {1-2},
  Pages                    = {91-6},
  Volume                   = {20},

  Abstract                 = {The author establishes a theoretical framework for analyzing and classifying actuators that generate their output by rectifying small-amplitude mechanical vibrations, such as might be generated by piezoelectric elements. These ideas are of special interest when designing for microfabrication, because motors based on these principles: can generate translational output directly without use of rotary bearings; appear to be scalable over several orders of magnitude of the length scale; and appear to be capable of generating mechanical power proportional to driving frequency over one to two orders of magnitude of frequency. In order to achieve this performance, it is necessary to be able to fabricate features, or at least control surface irregularities, on the scale v/w where v is velocity of the actuator and w is the operating frequency. (10 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:10 +0000}
}

