@Article{DS-JKH:99,
  Title                    = {{C}onstant spacing strategies for platooning in automated highway systems},
  Author                   = {{D}. {S}waroop and {J}. {K}. {H}edrick},
  Journal                  = asmeJDSMC,
  Year                     = {1999},
  Number                   = {3},
  Pages                    = {462-70},
  Volume                   = {121},

  Abstract                 = {An important aspect of an automated highway system is automatic vehicle following. Automatic vehicle follower systems must address the problem of string stability, i.e., the problem of spacing error propagation, and in some cases, amplification upstream from one vehicle to another due to same disturbance at the head of the platoon. An automatic vehicle following controller design that is (asymptotically) stable for one vehicle following another is not necessarily (asymptotically) stable for a string of vehicles. The dynamic coupling between vehicles in such close-formation platoons is a function of the available information (communicated as well as sensed), decentralized feedback control laws and the vehicle spacing policy in use. In the first half of the paper we develop a framework for establishing conditions for stability of the string in the presence of such dynamic interactions. We then develop a metric for analyzing the performance of a platoon resulting from different vehicle following control algorithms. This metric is the guaranteed rate of attenuation/non-amplification of spacing errors from one vehicle to another. In the latter half of the paper, we outline and analyze various constant spacing vehicle follower algorithms. All these algorithms are analyzed for sensing/actuation lags. (34 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

@Article{DS-JKH:96,
  Title                    = {{S}tring stability of interconnected systems},
  Author                   = {{D}. {S}waroop and {J}. {K}. {H}edrick},
  Journal                  = tac,
  Year                     = {1996},
  Number                   = {3},
  Pages                    = {349-57},
  Volume                   = {41},

  Abstract                 = {Introduces the notion of string stability of a countably infinite interconnection of a class of nonlinear systems. Intuitively, string stability implies uniform boundedness of all the states of the interconnected system for all time if the initial states of the interconnected system are uniformly bounded. It is well known that the input output gain of all the subsystems less than unity guarantees that the interconnected system is input-output stable. The authors derive sufficient ("weak coupling") conditions which guarantee the asymptotic string stability of a class of interconnected systems. Under the same "weak coupling" conditions, string-stable interconnected systems remain string stable in the presence of small structural/singular perturbations. In the presence of parameter mismatch, these "weak coupling" conditions ensure that the states of all the subsystems are all uniformly bounded when a gradient-based parameter adaptation law is used and that the states of all the systems go to zero asymptotically. (32 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:09 +0000}
}

@Article{PFS:85,
  Title                    = {{U}niform spherical coordinate quantization of spherically symmetric sources},
  Author                   = {{P}. {F}. {S}waszek},
  Journal                  = {{IEEE} {T}ransactions on {C}ommunications},
  Year                     = {1985},
  Number                   = {6},
  Pages                    = {518-21},
  Volume                   = {33},

  Abstract                 = {For signal quantization with the minimum mean square error criterion, optimum spherical coordinate quantizers have been shown to outperform optimum rectangular coordinate quantizers for many spherically symmetric sources. Spherical coordinate quantizers are designed by the author and analyzed under the added constraint that each scalar quantizer is a uniform (equal step-size) quantizer. It is shown that for small block length, uniform spherical coordinate quantizers outperform the uniform rectangular coordinate quantizer as well as the uniform quantizer consisting of a tesselation of the minimum inertia, space filling polytope. (15 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:10 +0000}
}

@Article{PFS-JBT:83,
