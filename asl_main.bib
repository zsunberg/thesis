@Article{HC-JWB:00,
  Title                    = {{S}ensor-based exploration: the hierarchical generalized {V}oronoi graph},
  Author                   = {{H}. {C}hoset and {J}. {B}urdick},
  Journal                  = ijrr,
  Year                     = {2000},
  Number                   = {2},
  Pages                    = {96-126},
  Volume                   = {19},

  Abstract                 = {The hierarchical generalized Voronoi graph (HGVG) is a roadmap developed for sensor-based exploration in unknown environments. The paper defines the HGVG structure. A robot can plan a path between two locations in its work space or configuration space by simply planning a path onto the HGVG, then along the HGVG, and finally from the HGVG to the goal. Since the bulk of the path planning occurs on the one-dimensional HGVG, motion planning in arbitrary dimensioned spaces is virtually reduced to a one-dimensional search problem. The bulk of the paper is dedicated to ensuring the HGVG is sufficient for motion planning by demonstrating that the HGVG (with its links) is an arc-wise connected structure. All of the proofs in the paper that lead toward the connectivity result focus on a large subset of spaces in R/sup 3/, but wherever possible, results are derived in R/sup m/. In fact, under a strict set of conditions, the HGVG (the GVG by itself) is indeed connected, and hence sufficient for motion planning. The chief advantage of the HGVG is that it possesses an incremental construction procedure that constructs the HGVG using only line-of-sight sensor data. Once the robot constructs the HGVG, it has effectively explored the environment, because it can then use the HGVG to plan a path between two arbitrary configurations. (22 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

@InProceedings{HC-JWB:94,
  Title                    = {{S}ensor based planning and nonsmooth analysis},
  Author                   = {{H}. {C}hoset and {J}. {W}. {B}urdick},
  Booktitle                = icra,
  Year                     = {1994},

  Address                  = {San Diego, CA},
  Month                    = may,
  Pages                    = {3034-3041},

  Abstract                 = {This paper describes some initial steps towards sensor based path planning in an unknown static environment. The method is a based on a sensor-based incremental construction of a one-dimensional retract of the free space. In this paper we introduce a retract termed the generalized Voronoi graph, and also analyze the roadmap of Canny and Lin's opportunistic path planner (1990, 1993). The bulk of this paper is devoted to the application of nonsmooth analysis to the Euclidean distance function. We show that the distance function is in fact nonsmooth at the points which are required to construct the plan. This analysis leads directly to the incorporation of simple and realistic sensor models into the planning scheme.},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:09 +0000}
}

