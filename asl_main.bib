@Article{GB-FB-JES-PP:02,
  Title                    = {{U}sing infrared sensors for distance measurement in mobile robots},
  Author                   = {{G}. {B}enet and {F}. {B}lanes and {J}. {E}. {S}im{\'o} and {P}. {P}{\'e}rez},
  Journal                  = jras,
  Year                     = {2002},
  Number                   = {4},
  Pages                    = {255-266},
  Volume                   = {40},

  Abstract                 = {The amplitude response of infrared (IR) sensors based on reflected amplitude of the surrounding objects is non-linear and depends on the reflectance characteristics of the object surface. As a result, the main use of IR sensors in robotics is for obstacle avoidance. Nevertheless, their inherently fast response is very attractive for enhancing the real-time operation of a mobile robot in, for instance, map building tasks. Thus, it seems that the development of new low-cost IR sensors able to accurately measure distances with reduced response times is worth researching. In this paper, a new IR sensor based on the light intensity back-scattered from objects and able to measure distances of up to 1m is described. Also, the sensor model is described and the expected errors in distance estimates are analysed and modelled. Finally, the experimental results obtained are discussed.},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:07 +0000}
}

