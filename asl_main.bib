@Article{DU:00a,
  Title                    = {{O}ptimal sensor location for parameter estimation of distributed processes},
  Author                   = {{D}. {U}ci{\'n}ski},
  Journal                  = ijc,
  Year                     = {2000},
  Number                   = {13},
  Pages                    = {1235-48},
  Volume                   = {73},

  Abstract                 = {The problem under consideration is to plan sensor movements in prescribed feasible regions in such a way as to maximize the accuracy of parameter identification of a distributed system defined in a two-dimensional spatial domain. A general functional defined on the Fisher information matrix is used as the design criterion. Central to the approach is the formulation of the problem as an optimal control one, possibly with state inequality constraints. Its solution is obtained numerically with the use of a method of successive linearization, which is capable of handling various constraints imposed on sensors' motions. Simple numerical examples are included which clearly demonstrate the ideas and trends presented in the paper. (44 References).},
  Annote                   = {Dept. of Robotics \& Software Eng., Tech. Univ. of Zielona Gora, Poland.},
  Date-added               = {2014-09-16 06:32:05 +0000},
