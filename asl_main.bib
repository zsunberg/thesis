@Article{AMB-NEL-JEM:00,
  Title                    = {{C}ontrolled {L}agrangians and the stabilization of mechanical systems. {I}. {T}he first matching theorem},
  Author                   = {{A}. {M}. {B}loch and {N}. {E}. {L}eonard and {J}. {E}. {M}arsden},
  Journal                  = tac,
  Year                     = {2000},
