@Article{HC-KN-NAL:03,
  Title                    = {{T}he arc-transversal median algorithm: {A} geometric approach to increasing ultrasonic sensor azimuth accuracy},
  Author                   = {{H}. {C}hoset and {K}. {N}agatani and {N}. {A}. {L}azar},
  Journal                  = tra,
  Year                     = {2003},
  Number                   = {3},
  Pages                    = {513-521},
  Volume                   = {19},

  Abstract                 = {This paper describes a new method for improving the azimuth accuracy of range information using conventional (Polaroid) low-resolution ultrasonic sensors mounted in a circular array on a mobile robot. Although ultrasonic sensors are fairly accurate in measuring distance in depth, they commonly have significant uncertainty in azimuth. We model this uncertainty with a uniform distribution along an arc. This means that the echo has an equal likelihood of originating from any point along the arc. We then introduce a new method to fuse sonar data to better approximate the actual obstacle location. This new method is termed the arc transversal median method because the robot determines the location of an object 1) by intersecting one arc with other arcs, 2) then by considering only "transversal" intersections, those which exceed a threshold in angle, and 3) by taking the median of the intersections. The median is a robust estimator that is insensitive to noise; a few stray readings will not affect its value. We show, via some simple geometric relationships, that this method can improve the azimuth accuracy of the sonar sensor by a specified amount under well-defined conditions. Experimental results on an ultrasonic sensor array situated on a mobile robot verify this approach.},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:07 +0000},
  Indexterms               = {mobile robots sonar ultrasonic transducers arc-transversal median algorithm geometric approach mobile robot range information robust estimator ultrasonic sensor array ultrasonic sensor azimuth accuracy}
}

@Article{HC-SW-KEA-JWB:00,
  Title                    = {{S}ensor-based exploration: incremental construction of the hierarchical generalized {V}oronoi graph},
  Author                   = {{H}. {C}hoset and {S}. {W}alker and {K}. {E}iamsa-{A}rd and {J}. {B}urdick},
  Journal                  = ijrr,
  Year                     = {2000},
  Number                   = {2},
  Pages                    = {126-48},
  Volume                   = {19},

  Abstract                 = {Prescribes an incremental procedure to construct roadmaps of unknown environments. Recall that a roadmap is a geometric structure that a robot uses to plan a path between two points in an environment. If the robot knows the roadmap, then it knows the environment. Likewise, if the robot constructs the roadmap, then it has effectively explored the environment. The paper focuses on the hierarchical generalized Voronoi graph (HGVG). The incremental construction procedure of the HGVG requires only local distance sensor measurements, and therefore the method can be used as a basis for sensor-based planning algorithms. Simulations and experiments using a mobile robot with ultrasonic sensors verify this approach. (15 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

