@Article{JB-JCL:93,
  Title                    = {{N}onholonomic {M}ultibody {M}obile {R}obots: {C}ontrollability and {M}otion {P}lanning in the {P}resence of {O}bstacles},
  Author                   = {{J}. {B}arraquand and {J}.-{C}. {L}atombe},
  Journal                  = {{A}lgorithmica},
  Year                     = {1993},
  Number                   = {2-3-4},
  Pages                    = {121-155},
  Volume                   = {10},

  Abstract                 = {We consider mobile robots made of a single body (car-like robots) or several bodies (tractors towing several trailers sequentially hooked). These robots are known to be nonholonomic, i.e.~they are subject to non-integrable equality kinematic constraints involving the velocity. in other words, the number of controls (dimension of the admissible velocity space) is smaller than the dimension of the configuration space. In addition, the range of possible controls is usually further constrained by inequality constraints due to mechanical stops in the steering mechanism of the tractor. We first analyze the controllability of such nonholonomic multibody robots. We show that the well-known Controllability Rank Condition Theorem is applicable to these robots even when there are inequality constraints on the velocity, in addition to the equality constraints. This allows us to subsume and generalize several controllability results recently published in the Robotics literature concerning nonholonomic mobile robots, and to infer several new important results. We then describe an implemented planner inspired by these results. We give experimental results obtained with this planner that illustrate the theoretical results previously developed.},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:09 +0000}
}

@Article{JB-JCL:91,
  Title                    = {{R}obot {M}otion {P}lanning: {A} distributed representation approach},
  Author                   = {{J}. {B}arraquand and {J}-{C}. {L}atombe},
  Journal                  = ijrr,
  Year                     = {1991},
  Number                   = {6},
  Pages                    = {628-649},
  Volume                   = {10},

  Abstract                 = {We propose a new approach to robot path planning which consists of building and searching a graph connecting the local minima of a potential function defined over the robot's configuration space. A planner based on this approach has been implemented. This planner is considerably faster than previous path planners and it solves problems for robots with many more degrees of freedom (DOF's). The power of the planner derives both from the ``good'' properties of the potential function and from the efficiency of the techniques used to escape the local minima of this function. The most powerful of these techniques is a Monte-Carlo technique that escapes local minima by executing Brownian motions. The overall approach is made possible by the systematic use of distributed representations (bitmaps) for both the robot's workspace and configuration space. We have experimented with the planner using several computer-simulated robots, including rigid objects with 3 DOF's (in 2D workspace) and 6 DOF's (in 3D workspace) and manipulator arms with 8, 10, and 31 DOF's (in 2D and 3D workspaces). Some of the most significant experiments are reported in this paper.},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:10 +0000}
}

