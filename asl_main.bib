@Article{KS-IS:96,
  Title                    = {{D}istributed algorithms for formation of geometric patterns with many mobile robots},
  Author                   = {{K}. {S}ugihara and {I}. {S}uzuki},
  Journal                  = jrs,
  Year                     = {1996},
  Number                   = {3},
  Pages                    = {127-39},
  Volume                   = {13},

  Abstract                 = {Discusses a method for controlling a group of mobile robots in a distributed manner. The method is distributed in the sense that all robots, or most of the robots in some cases, plan their motion individually based upon the given goal of the group and the observed positions of other robots. The authors illustrate the method by showing how a large number of robots can form an approximation of a circle, a simple polygon, or a line segment in the plane. The authors also show how the robots can distribute themselves nearly uniformly within a circle or a convex polygon in the plane. Finally, the authors show how the robots can be divided into two or more groups. It turns out that in many cases most robots execute an identical, simple algorithm. The performance of the method is demonstrated by simulation. (47 References).},
