  Journal                  = {{N}onlinearity},
  Year                     = {1998},
  Number                   = {5},
  Pages                    = {1365-8},
  Volume                   = {11},

  Abstract                 = {In this short note we uncover the geometry behind the classical result on averaging high-frequency vibrations x+a(1/ epsilon )V/sup t/(x)=0. It is shown that the classical effective potential of Kapitsa is produced by a centrifugal force of a point mass constrained to a certain curve determined by V. (4 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

