@Article{DU:00b,
  Title                    = {{O}ptimal selection of measurement locations for parameter estimation in distributed processes},
  Author                   = {{D}. {U}ci{\'n}ski},
  Journal                  = amcs,
  Year                     = {2000},
  Number                   = {2},
  Pages                    = {357-79},
  Volume                   = {10},

  Abstract                 = {The problem of locating pointwise sensor measurements so as to optimally estimate unknown parameters in a class of distributed systems is studied. Based on a scalar measure of performance defined on the corresponding Fisher information matrix, two approaches are developed for this problem: introduction of continuous designs, which allows for adaptation of well-known sequential algorithms of classical optimum experimental design, and application of standard nonlinear programming techniques. In each case, particular algorithms are delineated and analysis of the appropriate sensor placements is made. The relative advantages and shortcomings of both the approaches are discussed and demonstrated by applying them to a two-dimensional diffusion process. (45 References).},
  Date-added               = {2014-09-16 06:32:05 +0000},
  Date-modified            = {2014-09-16 06:32:08 +0000}
}

@InProceedings{DU-JK:99,
